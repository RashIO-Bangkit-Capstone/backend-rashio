{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": ["jsdoc"],
  "rules": {
    "linebreak-style": "off",
    "class-methods-use-this": "off",
    "jsdoc/no-undefined-types": 1
  }
}
